{
  "name": "portfolio-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href https://henrylangenhoven.github.io/",
    "build:watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "format:check": "prettier --config ./.prettierrc --list-different .",
    "format:all": "npx prettier --config ./.prettierrc --write . --check",
    "format:src": "npx prettier --config ./.prettierrc --write ./src --check",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.2",
    "@angular/common": "^21.0.2",
    "@angular/compiler": "^21.0.2",
    "@angular/core": "^21.0.2",
    "@angular/forms": "^21.0.2",
    "@angular/platform-browser": "^21.0.2",
    "@angular/platform-browser-dynamic": "^21.0.2",
    "@angular/router": "^21.0.2",
    "@standard-schema/spec": "^1.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.0.1",
    "@angular-eslint/eslint-plugin": "^21.0.1",
    "@angular-eslint/eslint-plugin-template": "^21.0.1",
    "@angular-eslint/schematics": "^21.0.1",
    "@angular-eslint/template-parser": "^21.0.1",
    "@angular/build": "^21.0.1",
    "@angular/cli": "~21.0.1",
    "@angular/compiler-cli": "^21.0.2",
    "@types/jasmine": "~5.1.8",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.0",
    "@typescript-eslint/types": "^8.48.0",
    "@typescript-eslint/utils": "^8.48.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "typescript": "^5.9.3",
    "zone.js": "^0.16.0"
  }
}
